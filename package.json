    "url": "https://github.com/richard-unterberg/ts-react-vite-minimal-starter/issues"
  },
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx src/",
    "format:lint": "yarn format && yarn lint",
    "check:lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "check:prettier": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,scss,md}\"",
    "check": "yarn check:lint && yarn check:prettier",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@fontsource/inter": "^5.0.16",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "lucide-react": "^0.297.0",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.1",
    "tailwind-styled-components": "^2.2.0",
    "tailwindcss": "3.3.6",
    "vite": "^5.0.10"
  },
  "devDependencies": {
    "vite-tsconfig-paths": "^4.2.2",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
